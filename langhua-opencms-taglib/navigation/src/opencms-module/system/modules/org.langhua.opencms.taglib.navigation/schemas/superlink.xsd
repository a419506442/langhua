		</xsd:sequence>
		<xsd:attribute name="language" type="OpenCmsLocale" use="optional"/>
	</xsd:complexType>
	<xsd:annotation>
		<xsd:appinfo>
			<resourcebundle name="org.langhua.opencms.taglib.navigation.message"/>
                        <layouts>				
				<layout element="ID" widget="SelectorWidget" configuration="1*|2" />
			</layouts>			
		</xsd:appinfo>
	</xsd:annotation>  
</xsd:schema>